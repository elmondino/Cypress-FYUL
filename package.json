{
  "name": "fyul-cypress-tests",
  "version": "1.0.0",
  "description": "Enterprise-grade Cypress test suite for FYUL.com",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:headed": "cypress run --headed",
    "open": "cypress open",
    "clean": "rimraf cypress/screenshots cypress/videos cypress/reports",
    "lint": "eslint 'cypress/**/*.ts'",
    "lint:fix": "eslint 'cypress/**/*.ts' --fix",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cypress",
    "e2e",
    "testing",
    "automation",
    "qa",
    "fyul"
  ],
  "author": "QA Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "axe-core": "^4.11.1",
    "cross-env": "^10.1.0",
    "cypress": "^15.8.2",
    "cypress-axe": "^1.7.0",
    "cypress-mochawesome-reporter": "^3.8.4",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^2.15.2",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.3.2",
    "rimraf": "^5.0.10",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/fyul-cypress-tests.git"
  }
}
