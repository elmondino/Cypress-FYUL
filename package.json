{
  "name": "fyul-cypress-tests",
  "version": "1.0.0",
  "description": "Enterprise-grade Cypress test suite for FYUL.com",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --config-file cypress.config.ts",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:edge": "cypress run --browser edge",
    "test:headed": "cypress run --headed",
    "test:mobile": "cypress run --config viewportWidth=375,viewportHeight=667",
    "test:tablet": "cypress run --config viewportWidth=768,viewportHeight=1024",
    "test:desktop": "cypress run --config viewportWidth=1920,viewportHeight=1080",
    "test:home": "cypress run --spec 'cypress/e2e/home/**/*.cy.ts'",
    "test:about": "cypress run --spec 'cypress/e2e/about/**/*.cy.ts'",
    "test:leadership": "cypress run --spec 'cypress/e2e/leadership/**/*.cy.ts'",
    "test:privacy": "cypress run --spec 'cypress/e2e/privacy/**/*.cy.ts'",
    "test:responsive": "cypress run --spec 'cypress/e2e/cross-browser/**/*.cy.ts'",
    "test:visual": "cypress run --spec 'cypress/e2e/visual/**/*.cy.ts'",
    "test:a11y": "cypress run --env a11y=true",
    "test:staging": "cross-env CYPRESS_ENV=staging cypress run",
    "test:dev": "cross-env CYPRESS_ENV=development cypress run",
    "open": "cypress open --config-file cypress.config.ts",
    "open:chrome": "cypress open --browser chrome",
    "open:firefox": "cypress open --browser firefox",
    "open:edge": "cypress open --browser edge",
    "clean": "rimraf cypress/screenshots cypress/videos cypress/reports",
    "clean:reports": "rimraf cypress/reports",
    "lint": "eslint 'cypress/**/*.ts'",
    "lint:fix": "eslint 'cypress/**/*.ts' --fix",
    "typecheck": "tsc --noEmit",
    "pretest": "npm run clean",
    "ci": "npm run clean && npm run test -- --record",
    "ci:chrome": "npm run test:chrome -- --record",
    "ci:firefox": "npm run test:firefox -- --record",
    "percy": "percy exec -- cypress run --spec 'cypress/e2e/visual/**/*.cy.ts'"
  },
  "keywords": [
    "cypress",
    "e2e",
    "testing",
    "automation",
    "qa",
    "fyul"
  ],
  "author": "QA Team",
  "license": "MIT",
  "devDependencies": {
    "@percy/cli": "^1.31.7",
    "@percy/cypress": "^3.1.7",
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "axe-core": "^4.11.1",
    "cross-env": "^10.1.0",
    "cypress": "^15.8.2",
    "cypress-axe": "^1.7.0",
    "cypress-mochawesome-reporter": "^3.8.4",
    "eslint": "^8.57.1",
    "eslint-plugin-cypress": "^2.15.2",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^4.4.1",
    "mochawesome-report-generator": "^6.3.2",
    "rimraf": "^5.0.10",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/fyul-cypress-tests.git"
  }
}
